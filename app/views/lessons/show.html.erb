<h1><%= @kindergarten.name %></h1>

<div class="row">
  <div class="col s12 m6">
    <div class="card blue-grey darken-1">
      <div class="card-content white-text">
        <span class="card-title"><%= @lesson.subject %></span>
        <p>Teacher: <%= @lesson.teacher %></p>
        <p>Official Bully: <%= @lesson.bully %></p>
        <p>Official Bullied: <%= @lesson.bullied %></p>
      </div>
      <div class="card-action">
        <%= link_to 'Home', root_path %>
        <%= link_to 'Students', lesson_sutdents_path(@lesson) %>
      </div>
    </div>
  </div>
</div>
